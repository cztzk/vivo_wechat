/* pages/my/my.wxss */

.user_box {
  width: 750rpx;
  background: url("data:image/png;base64,");
  background-size: cover;
  text-align: center;
  padding: 40rpx 0;
}

.avatarUrl .avatarImg {
  width: 120px;
  height: 120px;
  overflow: hidden;
  border-radius: 50px;
  margin: 0 auto;
}

.avatarUrl  .avatarImg {
  margin: 0 auto;
  width: 120rpx;
  height: 120rpx;
  display: block;
}

.nick_box {
  margin: 15rpx 0;
}

.nick {
  color: #fff;
}

.user_head_btn text {
  padding-right: 15rpx;
  color: #fff;
}

.user_head_btn text:last-child {
  padding-right: 0;
}

.weui-cell__hd {
  padding-right: 5rpx;
}

.iconfont {
  font-size: 40rpx;
}

.form {
  background: #fff;
}

.form .input_box {
  display: flex;
  justify-content: space-around;
  align-items: center;
  border-bottom: 1px solid #aaa;
  padding: 20rpx 30px;
}

.form .input_box label {
  width: 190rpx;
  font-size: 30rpx;
}

.form .input_box input {
  width: 500rpx;
  font-size: 30rpx;
}

.code_box {
  position: relative;
}

.code_box input {
  width: 340rpx;
}

.getcode {
  position: absolute;
  right: 30rpx;
  z-index: 99;
}

.msg {
  padding: 30rpx 0;
  text-align: center;
  font-size: 30rpx;
  color: #ce3c39;
}

.form_btn {
  padding: 0 30rpx 30rpx;
  text-align: center;
}

.form_btn>view {
  padding: 15rpx 0;
  color: #fff;
  background: #aaa;
}

.form_btn.active>view {
  background: #ce3c39;
}

.user_list .user_item {
  padding: 15rpx 25rpx;
  background: #fff;
  border-bottom: 1px solid #ddd;
}
.user_list .user_item:last-child{
  border-bottom: none;
}

.user_item .icon {
  width: 50rpx;
}

.user_item .user_item_name {
  width: 600rpx;
}
